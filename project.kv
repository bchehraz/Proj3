<Project>:
    color: 1,0,1,0   
	AnchorLayout: 
		anchor_x: 'center'
		anchor_y: 'bottom'
		orientation: 'horizontal'
		spacing: 10
        BoxLayout:
    		id: _sound_controls
    		size_hint: 1, None
    		height: 48   	
            orientation: 'horizontal'
            spacing: 2.5
            padding: 5
            Button:
                id: string1
                text: root.string1text
                on_press: root.changeFreqText(0)
                on_release: root.string1Callback()
            Button:
                id: string2
                text: root.string2text
                on_press: root.changeFreqText(1)
                on_release: root.string2Callback()
            Button:
                id: string3
                text: root.string3text
                on_press: root.changeFreqText(2)
                on_release: root.string3Callback()
            Button: 
                id: string4
                text: root.string4text
                on_press: root.changeFreqText(3)
                on_release: root.string4Callback()
            Button:
                id: string5
                text: root.string5text
                on_press: root.changeFreqText(4)
                on_release: root.string5Callback()
            Button: 
                id: string6
                text: root.string6text
                on_press: root.changeFreqText(5)
                on_release: root.string6Callback()
    BoxLayout:
	    Label:
	    	text: root.freqText
		Label:
	    	text: root.inputText
    AnchorLayout:
        anchor_y: 'top'
        anchor_x: 'center'
        Image:
            source: 'Dimetune.png'
            keep_ratio: True
            size_hint_y: .45
            size_hint_x: .45
            pos_x: 100
            anchor_y: 'top'
            width: self.parent.width
            height: self.parent.width/self.image_ratio

